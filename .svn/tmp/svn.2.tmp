<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.jx.blackface</groupId>
	<artifactId>mycenter</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>war</packaging>
	<name>mycenter</name>
	
	<dependencies>
		<dependency>
			<groupId>com.jx.blackface</groupId>
			<artifactId>servicecoreclient</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.jx.spat</groupId>
			<artifactId>argo</artifactId>
			<version>1.0.1</version>
		</dependency>
		<!-- <dependency>
			<groupId>com.jx.blackface</groupId>
			<artifactId>messageplug</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency> -->
		<dependency>
		<groupId>com.jx.blackface.gaea</groupId>
		<artifactId>vendor-service</artifactId>
		<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
            <groupId>javax.servlet</groupId>
            <artifactId>javax.servlet-api</artifactId>
            <scope>provided</scope>
            <version>3.0.1</version>
        </dependency>
        <dependency>
	        <groupId>com.jx.blackface</groupId>
	  		<artifactId>orderplug</artifactId>
	  		<version>0.0.1-SNAPSHOT</version>
        </dependency>
        <dependency>
		  <groupId>commons-beanutils</groupId>
		  <artifactId>commons-beanutils</artifactId>
		  <version>1.8.3</version>
		</dependency>
        <dependency>
        	<groupId>com.jx.service</groupId>
        	<artifactId>newcore</artifactId>
        	<version>0.0.1-SNAPSHOT</version>
        </dependency>
        <!-- 阿里巴巴json解析包 start -->
		<dependency>
			<groupId>com.alibaba</groupId>
			<artifactId>fastjson</artifactId>
			<version>1.1.41</version>
		</dependency>
        <dependency>
        	<groupId>com.jx.service.enterprise</groupId>
			<artifactId>enterprise-contract</artifactId>
			<version>0.0.1-SNAPSHOT</version>
        </dependency>
        <dependency>
			<groupId>com.jx.scf</groupId>
			<artifactId>com.jx.scf.workflow-contract</artifactId>
			<version>0.0.1-SNAPSHOT</version>
        </dependency>
        <dependency>
		  <groupId>com.jx.blackface.gaea</groupId>
		  <artifactId>usercenter</artifactId>
		  <version>0.0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.jx.blackface.tools</groupId>
			<artifactId>webblack</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.jx.tools</groupId>
			<artifactId>WAQ</artifactId>
			<version>0.0.2-SNAPSHOT</version>
		</dependency>
		<!-- 商品库客户端调用服务 -->
		<dependency>
			<groupId>com.jx.blackface.gaea</groupId>
			<artifactId>sell-contract</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<!-- 消息 -->
		<dependency>
		  <groupId>com.jx.service</groupId>
		  <artifactId>messagecenter</artifactId>
		  <version>0.0.3-SNAPSHOT</version>
		</dependency>
		<!-- 阿里云开放存储 SDK start -->
		<dependency>
			<groupId>com.aliyun.oss</groupId>
			<artifactId>aliyun-sdk-oss</artifactId>
			<version>2.0.2</version>
		</dependency>
		<!-- 阿里云开放存储 SDK end -->
		<dependency>
			<groupId>org.apache.ant</groupId>
			<artifactId>apache-ant-zip</artifactId>
			<version>1.8.0</version>
		</dependency>
		
		<dependency>
			<groupId>org.jsoup</groupId>
			<artifactId>jsoup</artifactId>
			<version>1.7.2</version>
		</dependency>
		<!-- track log -->
		<dependency>
			<groupId>com.jx.blackface.tools</groupId>
			<artifactId>blackTrack</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<!-- 文件操作插件 -->
		<dependency>
	        <groupId>com.jx.blackface</groupId>
	  		<artifactId>fileplug</artifactId>
	  		<version>0.0.1-SNAPSHOT</version>
        </dependency>
	</dependencies>
	<build>
		<finalName>mycenter</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<!-- <version>0.0.1</version> -->
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<systemProperties>
						<property>
							<name>log4j.configuration</name>
							<value>log4j.properties</value>
						</property>
					</systemProperties>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.1.1</version>
				<configuration>
					<failOnMissingWebXml>false</failOnMissingWebXml>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.5.1</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<!-- http://wiki.eclipse.org/Jetty/Feature/Jetty_Maven_Plugin -->
				<groupId>org.mortbay.jetty</groupId>
				<artifactId>jetty-maven-plugin</artifactId>
				<version>8.0.0.M3</version>
				<configuration>
					<stopPort>9966</stopPort>
					<stopKey>foo</stopKey>
					<scanIntervalSeconds>0</scanIntervalSeconds>
					<connectors>
						<connector implementation="org.eclipse.jetty.server.nio.SelectChannelConnector">
							<port>8080</port>
							<maxIdleTime>60000</maxIdleTime>
						</connector>
					</connectors>
					<webAppConfig>
						<defaultsDescriptor>src/main/resources/webdefault.xml</defaultsDescriptor>
						<contextPath>/</contextPath>
					</webAppConfig>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.5.1</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<configuration>
					<url>http://192.168.1.101:9987/manager/text</url>
					<server>tomcat-www</server>
					<username>tomcat</username>
					<password>tomcat</password>
					<path>/</path>
				</configuration>
			</plugin>

		</plugins>
	</build>

</project>