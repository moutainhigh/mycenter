                                        <div class="n-select w-160" name="n-select" />
                                            <input class="n-val" type="text" value="请选择"  readonly>
                                            <ul></ul>
                                        </div>
                                        
                                        <input type="hidden" placeholder="产生方式" id="posBrForm" data-validate="required:true;" data-select="none" data-info="partnerInfo:ext" />
                                		<input type="hidden" placeholder="任职期限" id="offYears" data-validate="required:true;" data-select="none" data-info="partnerInfo:ext" />
                                
                                    </td>
                                </tr>
                                
